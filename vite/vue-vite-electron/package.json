{
  "name": "vue-vite-electron",
  "version": "0.0.1",
  "author": "xcr",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "dev:e": "node scripts/watch.mjs",
    "build:e": "node scripts/build.mjs && electron-builder",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "main": "dist/main/index.cjs",
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "vite",
    "vue3",
    "electron"
  ],
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "chalk": "^5.0.0",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-eslint": "^1.3.0",
    "vue-tsc": "^0.29.8"
  },
  "env": {
    "HOST": "127.0.0.1",
    "PORT": 3333
  }
}
